  "scripts": {
    "dev:parallel": "cross-env yarn run parallel-webpack --watch -p=4",
    "dev": "yarn dev:fb & yarn dev:default",
    "dev:fb": "cross-env BROWSERSLIST_ENV=fb yarn encore dev --watch",
    "dev:default": "cross-env yarn encore dev --watch & yarn workspace storescreen run dev",
    "prod": "yarn prod:default && yarn prod:fb",
    "prod:fb": "BROWSERSLIST_ENV=fb yarn encore production",
    "prod:default": "yarn encore production && yarn workspace storescreen run prod",
    "staging": "yarn staging:default && yarn staging:fb",
    "staging:fb": "BROWSERSLIST_ENV=fb yarn encore production --node-env staging",
    "staging:default": "yarn encore production --node-env staging && yarn workspace storescreen run staging",
    "pre-production": "yarn pre-production:default && yarn pre-production:fb",
    "pre-production:fb": "BROWSERSLIST_ENV=fb yarn encore production --node-env pre-production",
    "pre-production:default": "yarn encore production --node-env pre-production && yarn workspace storescreen run pre-production",
    "test": "jest --reporters=default",
    "test:ci": "yarn jest --config ./jest.config.js --collectCoverage --coverageDirectory=\"<rootDir>/build/jest\"  --ci  --watchAll=false",
    "test-style": "stylelint **/*.scss",
    "fix-style": "stylelint **/*.scss --fix",
    "cypress": "./node_modules/.bin/cypress run",
    "cypress-headed": "./node_modules/.bin/cypress run --headed",
    "cypress-open": "./node_modules/.bin/cypress open"
  },
